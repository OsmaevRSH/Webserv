server   {
	port 5555;
	server_name ltheresi.team;

	location / {
		autoindex on;
		root ./Test/conf;
		max_body_size 55;
		index ./Test/conf;
		location /html/page {
            autoindex on;
            root ./Test/dir;
            max_body_size 100;
            index ./Test/conf;
            location /html/page/var/ {
                autoindex on;
                root ./Test/dir;
                max_body_size 666;
                index ./Test/conf;
                allow GET;
            }
        }
	}
}