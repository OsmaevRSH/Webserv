server   {
	ip 127.0.0.1;
	port 5555;
	server_name ltheresi.team;

	location / {
		autoindex on;
		root ./App/Html;
		max_body_size 55;
		index index3.html index2.html;
		location /html {
			autoindex on;
			root ./App/Html;
			max_body_size 100;
			index index2.html;
			location /html/ttt {
				autoindex on;
				root ./App/Html;
				max_body_size 100;
				index index3.html;
			}
		}
	}
	location /abs {
		autoindex on;
		root ./App/Html;
		max_body_size 55;
		index index2.html;
		location /html {
			autoindex on;
			root ./App/Html;
			max_body_size 100;
			index index3.html;
		}
	}
}

error_page 404 ./App/Error_page/404.html;