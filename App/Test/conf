server   {
	ip 127.0.0.1;
	port 5555;
	server_name ltheresi.team;

	route / {
		autoindex on;
		root ./App/Html;
		max_body_size 55;
		index index1.html index2.html;
		route /html {
			autoindex on;
			root ./App/Html;
			max_body_size 100;
			index index2.html;
			route /html/ttt {
				autoindex on;
				root ./App/Html;
				max_body_size 100;
				index index3.html;
			}
		}
	}
	route /abs {
		autoindex on;
		root ./App/Html;
		max_body_size 55;
		index index2.html;
		route /html {
			autoindex on;
			root ./App/Html;
			max_body_size 100;
			index index3.html;
		}
	}
}

