	ip 127.0.0.1;
	port 5555;
	server_name ltheresi.team;

	location / {
		allow GET;
		autoindex on;
		root ./Tester/YoupiBanane;
	}
	location /directory/ {
		allow GET;
		autoindex off;
		index youpi.bad_extension;
		root ./Tester/YoupiBanane;
	}
	location /put_test/ {
		allow PUT;
		autoindex off;
		index youpi.bad_extension;
		root ./Tester/YoupiBanane;
	}
}
